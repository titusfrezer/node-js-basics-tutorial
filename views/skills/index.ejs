<%- include('../partials/header'); -%>
<link href="/css/data-table.css" rel="stylesheet" />
<main id="main" class="main">
  <div class="pagetitle">
    <h1>Skills</h1>
    <nav>
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
        <li class="breadcrumb-item">Tables</li>
        <li class="breadcrumb-item active">Data</li>
      </ol>
    </nav>
  </div>
  <!-- End Page Title -->

  <section class="section">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-body">
            <a href="/add/skill"
              ><h5 class="card-title">Add Skills</h5></a
            >

            <!-- Table with stripped rows -->
            <table class="table datatable">
              <thead>
                <tr>
                  <!-- <th scope="col">#</th> -->
                  <th scope="col">Skill Name</th>
                  <th scope="col">Average(From 100)</th>
                  <th scope="col">Skill Type</th>
                </tr>
              </thead>
              <tbody>
                <% if(skills.length>0){%>
                   <% skills.forEach(sk=>{%>

                <tr>
                  <!-- <th scope="row">1</th> -->
                  <td><%= sk.skillName %></td>
                  <td><%= sk.skillAverage %></td>
                  <td><%= sk.skillType %></td>
                 
                </tr>
                <%})%> <% } %>
              </tbody>
            </table>
            <!-- End Table with stripped rows -->
          </div>
        </div>
      </div>
    </div>
  </section>
</main>
<!-- End #main -->

<script src="/js/data-table.js"></script>
<script src="/js/tinymce.min.js"></script>
<%- include('../partials/footer'); -%>
